/* pages/index4/index4.wxss */
.center{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
  width: 400rpx;
  height: 400rpx;
}

.url{
  margin: 0 auto;
  width: 200rpx;
  height: 200rpx;
  overflow: hidden;
  border-radius: 50%;
}

.name{
  margin: 0 auto;
  color: green;
  text-align: center;
  margin: 5rpx;
}

.h5{
  line-height: 60rpx;
  width: 400rpx;
  text-align: center;
}

.btn{
  margin: 0 auto;
  width: 300rpx;
  margin-top: 30rpx;
  padding: 15rpx;
  text-align: center;
  background-color:#fde6b4;
  border-radius: 5px;
  color: white;
}