swiper{
  width: 100%;
  height: 400rpx;
}
swiper image{
  width: 100%;
  height: 100%;
}
.btn{
  width: 300rpx;
  margin: 20px auto;
  background-color: #80c19f;
  color: white;
  border-radius: 20rpx;
  padding: 20rpx 0;
  text-align: center;
}
.fu{
  padding: 20rpx;
}
.fu>image{
  width: 100%;
  height: 260rpx;
  padding-bottom: 10rpx;
}
.fu>view{
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.fu>view>image{
  width: 48%;
  height: 260rpx;
  padding-bottom: 20rpx;
}
